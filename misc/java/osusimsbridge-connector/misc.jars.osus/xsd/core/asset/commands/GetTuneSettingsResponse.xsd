<xs:schema xmlns="commands.asset.core.th.dod.mil"
    targetNamespace="commands.asset.core.th.dod.mil"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
    xmlns:shared="http://th.dod.mil/core/types"
    elementFormDefault="qualified" jaxb:version="2.1">
    <xs:import namespace="http://th.dod.mil/core/types" schemaLocation="../../types/SharedTypes.xsd" />
    <xs:include schemaLocation="BaseTypes.xsd"></xs:include> 
  
    <xs:element name="getTuneSettingsResponse" type="GetTuneSettingsResponse" />
    <xs:complexType name="GetTuneSettingsResponse" >
        <xs:annotation>
            <xs:documentation>Get the current RF tune settings for an asset.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="Response">
                <xs:sequence>
                    <xs:element name="channelConfiguration" type="shared:ChannelConfiguration" minOccurs="0" 
                        maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>
                                RF tune configuration for a single set of channels.  If nothing is configured, there
                                will not be any configurations listed.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
</xs:schema>
