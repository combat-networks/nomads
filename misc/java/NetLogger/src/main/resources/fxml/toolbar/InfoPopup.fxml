<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXRippler?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<?import com.jfoenix.controls.JFXPopup?>
<JFXPopup xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="netlogger.controller.InfoPopupController">

<StackPane styleClass="info-popup" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
        <VBox spacing="5.0">
            <JFXRippler>
                <Label fx:id="_contactInfoLabel" text="Contact Info" styleClass="info-label">
                    <padding>
                        <Insets bottom="5.0" left="15.0" right="15.0" top="5.0" />
                    </padding>
                </Label>

            </JFXRippler>
            <JFXRippler>
                <Label fx:id="_settingsLabel" text="Settings" styleClass="info-label">
                    <padding>
                        <Insets bottom="5.0" left="15.0" right="15.0" top="5.0" />
                    </padding>
                </Label>
            </JFXRippler>
        </VBox>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
    </StackPane>
</JFXPopup>
