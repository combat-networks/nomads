<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="300.0" spacing="15.0" style="-fx-border-color: black;" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="netlogger.controller.SubjectCounter">
    <padding>
        <Insets top="5.0" />
    </padding>
    <Text text="Count of measure subjects">
      <font>
         <Font name="System Bold" size="14.0" />
      </font></Text>
    <VBox fx:id="_subjectsVBox">
         <VBox.margin>
            <Insets left="30.0" />
         </VBox.margin>
    </VBox>
</VBox>
